<templateSet group="Dart">
  <template name="ctor" value="/// $ClassName$ constructor&#10;$ClassName$({$GroovyArgs$})" description="Constructor generator" toReformat="false" toShortenFQNames="true">
    <variable name="ClassName" expression="dartClassName()" defaultValue="" alwaysStopAt="false" />
    <variable name="GroovyArgs" expression="groovyScript(&quot;C:\\Users\\brenn\\source\\repos\\brenn\\ishop\\groovy_args.groovy&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="DART" value="true" />
      <option name="FLUTTER" value="true" />
    </context>
  </template>
  <template name="extensionProperty" value="extension $ExtensionName$ on $Type$ {&#10;  $ReturnType$ get $propertyName$ {&#10;}&#10;}" description="Create property extension" toReformat="false" toShortenFQNames="true">
    <variable name="ExtensionName" expression="&quot;ExtensionName&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="Type" expression="&quot;Type&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="ReturnType" expression="&quot;ReturnType&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="propertyName" expression="&quot;propertyName&quot;" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>